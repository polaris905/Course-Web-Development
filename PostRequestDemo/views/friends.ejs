<h1>Friends List Goes Here!</h1>

<% varFriends.forEach(function(val) { %>
    <li>I have a friend named <%= val %>.</li>
<% }); %>

<form action = "/addfriend" method = "POST">
    <input type = "text" name = "newfriend" placeholder = "Please type your friend here.">
    <button>I made a new friend.</button>
</form>
